#include"i2c.h"
#include"Accelerometer.h"

void AccelerometerManagePower()
{
	HAL_I2C_Mem_Write(ACC_I2C_HANDLE,MPU6050_DEVICE_ADDRESS,
			MPU6050_RA_PWR_MGMT,1,g_MPU6050_PowerManagement1Settings,
			1,ACC_I2C_TIMEOUT);
}
